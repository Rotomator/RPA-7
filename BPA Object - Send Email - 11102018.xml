  <stage stageid="c194fd9f-8adb-417d-87d5-861925a62c70" name="Send Detailed Report" type="Code">
    <subsheetid>18766abf-1222-42ad-abf9-6821b471c479</subsheetid>
    <loginhibit />
    <narrative>
    </narrative>
    <displayx>15</displayx>
    <displayy>0</displayy>
    <displaywidth>60</displaywidth>
    <displayheight>30</displayheight>
    <font family="Segoe UI" size="10" style="Regular" color="000000" />
    <inputs>
      <input type="text" name="CC_MailID" expr="[CC_MailID]" />
      <input type="text" name="InputFileName" expr="[folderPath]&amp;&quot;\&quot;&amp;[inClientName]&amp;&quot;.xlsx&quot;" />
      <input type="text" name="Mail_Subject" expr="&quot;Payment Reconciliation Report &quot;&amp;FormatDateTime(Today(), &quot;dd MMM yyyy&quot;)&amp;&quot; - &quot;&amp;[inClientName]&amp;&quot; - &quot;&amp;[successMessage]" />
      <input type="date" name="dateSub" expr="FormatDateTime(Today(), &quot;dd MMM yyyy&quot;)" />
      <input type="text" name="Mail_Body" expr="[emailBody]" />
      <input type="text" name="To_MailID" expr="[TO_MailID]" />
      <input type="text" name="Message" expr="&quot;RA Processing Report&quot;" />
      <input type="time" name="startTime" expr="[processStartTime]" />
      <input type="time" name="endTime" expr="[processEndTime]" />
      <input type="timespan" name="executionTime" expr="[totalExecutionTime]" />
      <input type="text" name="clientName" expr="[inClientName]" />
      <input type="number" name="processedCount" expr="[processedCount]" />
      <input type="number" name="totalCount" expr="[totalCount]" />
      <input type="number" name="unProcessedCount" expr="[unProcessedCount]" />
      <input type="text" name="machineName" expr="[machineName]" />
      <input type="text" name="screeshotPath" expr="[screenShotPath]" />
      <input type="text" name="logFilePath" expr="[logFilePath]" />
    </inputs>
    <onsuccess>b2d45ad1-0def-46c7-8e9d-daa5d2211310</onsuccess>
    <code><![CDATA[
    Dim OutApp As Object
    Dim OutMail As Object
    Dim SoFile As String


    OutApp = Microsoft.VisualBasic.CreateObject("Outlook.Application")
    OutMail = OutApp.CreateItem(0)

   
   SoFile = InputFileName
   Mail_Body = Mail_Body.Replace("{Start Time}",startTime.ToString("hh:mm:ss")) 	
   Mail_Body = Mail_Body.Replace("{Completion Time}",endTime.ToString("hh:mm:ss"))
   Mail_Body = Mail_Body.Replace("{Execution Time}",executionTime.ToString()) 
   Mail_Body = Mail_Body.Replace("{clientName}",clientName)
   Mail_Body = Mail_Body.Replace("{Total Invoices}",totalCount)
   Mail_Body = Mail_Body.Replace("{Processed}",processedCount)
   Mail_Body = Mail_Body.Replace("{unprocessed}",unProcessedCount)
   Mail_Body = Mail_Body.Replace("{machineName}",machineName)
   Mail_Body = Mail_Body.Replace("{logFilePath}",logFilePath)
   Mail_Body = Mail_Body.Replace("{screeshotPath}",screeshotPath)
   Mail_Body = Mail_Body.Replace("{date}",dateSub)
   

    With OutMail
        .To = To_MailID
        .CC = CC_MailID
        .Subject = Mail_Subject
        .HtmlBody = Mail_Body 
		Try
		.Attachments.Add(SoFile)			'Input File						
		catch ex as Exception
		Message = ex.Message
		End Try
        .Display   'or use .Display
		.send
    End With

     OutMail = Nothing
     OutApp = Nothing
]]></code>
  </stage>
